type: txt

help: Set local filter name

create:expression: "sh -c \"echo create wlm=[$VAR(../../../@)] dir=[$VAR(..)] name=[$VAR(@)] \
>> /tmp/cli.log && \
sudo /opt/vyatta/sbin/vyatta-firewall.pl \
--update-interfaces update $VAR(../../../@) $VAR(..) $VAR(@)\" "

update:expression: "sh -c \"echo update wlm=[$VAR(../../../@)] dir=[$VAR(..)] name=[$VAR(@)] \
>> /tmp/cli.log && \
sudo /opt/vyatta/sbin/vyatta-firewall.pl \
--update-interfaces update $VAR(../../../@) $VAR(..) $VAR(@)\" "

delete:expression: "sh -c \"echo delete wlm=[$VAR(../../../@)] dir=[$VAR(..)] name=[$VAR(@)] \
>> /tmp/cli.log && \
sudo /opt/vyatta/sbin/vyatta-firewall.pl \
--update-interfaces delete $VAR(../../../@) $VAR(..) $VAR(@)\" "
